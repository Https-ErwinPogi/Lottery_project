<h1>Admin Users</h1>
<table class="table table-bordered table-hover">
  <thead>
  <th>Id</th>
  <th>email</th>
  <th>genre</th>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
    </tr>
  <% end %>
</table>